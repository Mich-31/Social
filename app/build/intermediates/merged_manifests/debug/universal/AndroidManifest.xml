            android:theme="@style/AppTheme" />
        <activity
            android:name="com.ubtrobot.mini.sdkdemo.BehaviorActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.ubtrobot.mini.sdkdemo.RobotInitActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.ubtrobot.mini.sdkdemo.DiscorverActivity"
